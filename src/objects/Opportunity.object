    <validationRules>
        <fullName>Must_choose_Organization_or_Primary_Con</fullName>
        <active>true</active>
        <description>User must choose to relate this opportunity to an Organization Name or a Primary Contact, which determines the hard credit attribution, both are not allowed.</description>
        <errorConditionFormula>AND( NOT(ISBLANK( AccountId )),NOT(ISBLANK(  ContactId__c  )) )</errorConditionFormula>
        <errorMessage>You may only set the Organization Name or the Contact Name to receive the hard credit attribution for this opportunity, but not both.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NonDeductible_Amt_not_more_than_Total</fullName>
        <active>true</active>
        <errorConditionFormula>Non_Deductible_Amount__c &gt; Amount</errorConditionFormula>
        <errorMessage>The Non-Deductible Amount cannot be greater than the total Amount of the Opportunity.</errorMessage>
    </validationRules>
