<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Assign an Engagement Level to a Contact based on the value of a field on a Contact record.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Field_Type__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Only values supported are checkbox and text. Text can correspond to a number of Salesforce field types that are text based, e.g. Text, Text Area, Picklist, Email, et al.</inlineHelpText>
        <label>Field Type</label>
        <length>50</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Field_Value__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The value of the field necessary to assign this level, e.g. True or False for checkbox type, or any text value for text type. An asterisk (*) corresponds to any non-empty value</inlineHelpText>
        <label>Field Value</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Fieldname__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The Developer name of the field, e.g. my_checkbox__c or my_textfield__c.</inlineHelpText>
        <label>Fieldname</label>
        <length>150</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Level_Value__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The number of the level earned by this field.</inlineHelpText>
        <label>Level Value</label>
        <precision>2</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Engagement Contact Field Settings</label>
</CustomObject>
