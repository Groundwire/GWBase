        <label>Job Change Wizard Relationship</label>
        <length>50</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Lead_Converter_Account_Name_Not_Provided__c</fullName>
        <defaultValue>&quot;[not provided]&quot;</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>The Organization name for Leads where the Organization is unknown. These will be added to the Individual account.</inlineHelpText>
        <label>Lead Converter Account Name Not Provided</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
