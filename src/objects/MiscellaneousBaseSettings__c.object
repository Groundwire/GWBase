<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Miscellaneous settings used by GWBase.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Campaign_Member_Status_Donated__c</fullName>
        <defaultValue>&quot;Donated&quot;</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>The Campaign Member Status value to use for a contact that donates due to a campaign.</inlineHelpText>
        <label>Campaign Member Status Donated</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Enable_Campaign_Member_Error_Table__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Enable the Campaign Member Error holding table, to hold and process failed Campaign Member inserts due to row locking errors.</description>
        <externalId>false</externalId>
        <label>Enable Campaign Member Error Table</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>GW_Help_Docs_Password__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Password for the Groundwire help documentation site.</inlineHelpText>
        <label>GW Help Docs Password</label>
        <length>50</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>GW_Help_Docs_Username__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Username for the Groundwire help documentation site.</inlineHelpText>
        <label>GW Help Docs Username</label>
        <length>100</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Individual_Account_Id__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>In order to increase code efficiency, set this to the 15 character Salesforce ID of the Individual account.</inlineHelpText>
        <label>Individual Account Id</label>
        <length>18</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Individual_Account_Name__c</fullName>
        <defaultValue>&quot;Individual&quot;</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>The name of the account that Contacts without an Organization should be added to.</inlineHelpText>
        <label>Individual Account Name</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Job_Change_Wizard_Relationship__c</fullName>
        <defaultValue>&quot;Employee&quot;</defaultValue>
        <externalId>false</externalId>
