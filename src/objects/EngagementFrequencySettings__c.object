<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Specify an Engagement Level to be set based on the frequency of a Contact&apos;s participation in Campaigns at a certain level, optionally only counting Campaigns with a certain field value.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Field_Value_1__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Optional. The value of Fieldname 1 required for a Campaign to count toward this rule. For checkboxes, &quot;True&quot; corresponds to checked.</inlineHelpText>
        <label>Field Value 1</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Fieldname_1__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Optional. The developer name of a field in Campaign to count toward this rule.</inlineHelpText>
        <label>Campaign Fieldname 1</label>
        <length>200</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Level_Required__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Optional. The minimum engagement level of a Campaign required to count towards this rule.</inlineHelpText>
        <label>Level Required</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Level_To_Assign__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The engagement level assigned to Contacts fitting these requirements.</inlineHelpText>
        <label>Level To Assign</label>
        <precision>2</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Number_of_Actions_Required__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The number of Campaigns that fit this criteria a Contact should have participated in to be assigned a new level.</inlineHelpText>
        <label>Number of Actions Required</label>
        <precision>2</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Engagement Frequency Settings</label>
</CustomObject>
