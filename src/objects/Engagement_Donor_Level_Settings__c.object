<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Specify the donation amounts that correspond to different levels on your organization&apos;s Engagement Pyramid.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Level__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The number of the Engagement Level this amount of giving corresponds to.</inlineHelpText>
        <label>Level</label>
        <precision>1</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Lower_Limit__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The minimum amount a donor must contribute (or pledge) in a year to be assigned the engagement level specified. Do not assign any a lower limit of $0 or less, or make tiers that overlap.</inlineHelpText>
        <label>Lower Limit</label>
        <precision>14</precision>
        <required>true</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <label>Engagement Donor Level Settings</label>
</CustomObject>
